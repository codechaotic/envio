{
  "name": "eznv",
  "version": "0.1.0",
  "engines": {
    "node": ">=6.4.0"
  },
  "description": "Env Management without the fuss",
  "author": "Asa Jones <thinkinfractals@gmail.com> (https://github.com/codechaotic)",
  "license": "ISC",
  "keywords": [
    "env",
    "environment",
    "dotenv",
    "config"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf {src/**/*,index}{.d.ts,.d.ts.map,.js,.js.map}",
    "lint": "tslint index.ts src/**/*.ts",
    "compile": "pegjs --plugin ts-pegjs -o src/parse.ts src/parse.pegjs",
    "coverage": "nyc report --reporter=html && opn ./coverage/index.html",
    "report-coverage": "nyc report --reporter=text-lcov | coveralls",
    "test": "nyc mocha --opts mocha.opts src/**/*.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/codechaotic/eznv"
  },
  "bugs": {
    "url": "https://github.com/codechaotic/eznv/issues"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "src/**/*.d.ts*",
      "src/**/*.spec.ts"
    ]
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.18",
    "@types/sinon": "^7.0.3",
    "@types/sinon-chai": "^3.2.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.2",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^13.1.0",
    "opn-cli": "^4.0.0",
    "pegjs": "^0.10.0",
    "rimraf": "^2.6.3",
    "sinon": "^7.2.2",
    "sinon-chai": "^3.3.0",
    "source-map-support": "^0.5.9",
    "ts-node": "^7.0.1",
    "ts-pegjs": "^0.2.2",
    "tslint": "^5.12.1",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.1.6"
  }
}
